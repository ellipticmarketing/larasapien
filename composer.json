{
    "name": "ellipticmarketing/larasapien",
    "type": "library",
    "authors": [
         {
            "name": "Rolando Rojas",
            "email": "rolando.rojas@ellipticmarketing.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "laravel/framework": ">=5.5.0 <=12.x-dev"
    },
    "require-dev": {
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4": {
            "EllipticMarketing\\Larasapien\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EllipticMarketing\\Larasapien\\LarasapienServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": true
    }
}
